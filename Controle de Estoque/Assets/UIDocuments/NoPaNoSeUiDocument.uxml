    </ui:VisualElement>
    <ui:VisualElement name="NewItemContainer" style="flex-grow: 0; background-color: rgb(93, 199, 209); position: absolute; width: 30%; height: 30%; top: 40%; left: 40%; justify-content: center; background-image: none; display: none; border-left-color: rgb(32, 42, 78); border-right-color: rgb(32, 42, 78); border-top-color: rgb(32, 42, 78); border-bottom-color: rgb(32, 42, 78); border-left-width: 10px; border-right-width: 10px; border-top-width: 10px; border-bottom-width: 10px;">
