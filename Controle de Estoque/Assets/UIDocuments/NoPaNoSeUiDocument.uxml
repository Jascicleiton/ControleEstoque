        <ui:TextField picking-mode="Ignore" value="Nome do novo item a ser adicionado" name="NewItemName" style="font-size: 20px; -unity-text-align: upper-center; margin-bottom: 10px; margin-left: 0; margin-right: 0; height: 50px; margin-top: 0;" />
        <ui:Label tabindex="-1" text="Quantidade" display-tooltip-when-elided="true" style="font-size: 20px; -unity-text-align: upper-center; margin-bottom: 5px; margin-left: 15px; margin-right: 15px; margin-top: 8px;" />
        <ui:TextField picking-mode="Ignore" value="Quantidade de itens que estão sendo adicionados" name="NewItemQuantity" style="font-size: 20px; -unity-text-align: upper-center; margin-bottom: 10px; margin-left: 0; margin-right: 0; height: 50px; margin-top: 0;" />
        <ui:Button text="Adicionar" display-tooltip-when-elided="true" name="AddNewItemButton" style="border-left-width: 0; border-right-width: 0; border-top-width: 0; border-bottom-width: 0; background-color: rgba(188, 188, 188, 0); background-image: url(&apos;project://database/Assets/Images/Buttons/Button%203.png?fileID=21300000&amp;guid=82e46b070298079439f5c112b07ebc6d&amp;type=3#Button 3&apos;); color: rgb(255, 255, 255); -unity-background-scale-mode: stretch-to-fill; height: 70px; font-size: 20px; width: 30%; align-self: center; margin-top: 0; padding-left: 3%; margin-left: 0; margin-right: 0; margin-bottom: 0;" />
    </ui:VisualElement>
    <ui:VisualElement name="MessagePanel" class="messagePanel">
        <ui:Label tabindex="-1" display-tooltip-when-elided="true" name="MessageLabel" class="messageLabel" />
        <ui:Button text="OK" display-tooltip-when-elided="true" name="MessageButton" class="messageButton" />
    </ui:VisualElement>
</ui:UXML>
